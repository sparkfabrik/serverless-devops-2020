{
  "name": "profile-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "test": "nyc mocha -r ts-node/register -r tsconfig-paths/register --config=test/.mocharc.yml",
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "tslint --project tsconfig.json --fix",
    "compile": "tsc -p .",
    "local": "sls offline start --stage=local --region=local",
    "deploy": "cd apis && sls deploy -v"
  },
  "dependencies": {
    "aws-sdk": "2.342.0",
    "aws-xray-sdk-core": "1.2.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.14",
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/mocha": "8.0.3",
    "@types/mock-fs": "4.10.0",
    "@types/node": "12.12.54",
    "@types/sinon": "4.3.3",
    "@types/sinon-chai": "2.7.29",
    "@types/uuid": "^8.3.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "gts": "^2.0.2",
    "mocha": "7.2.0",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "prettier": "2.1.1",
    "serverless": "2.4.0",
    "serverless-apigw-binary": "0.4.4",
    "serverless-domain-manager": "5.0.0",
    "serverless-plugin-scripts": "1.0.2",
    "serverless-webpack": "5.3.5",
    "sinon": "4.5.0",
    "sinon-chai": "2.14.0",
    "ts-loader": "8.0.3",
    "ts-mocha": "7.0.0",
    "tslint": "6.1.2",
    "typescript": "3.9.7",
    "webpack": "4.44.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
